            }
          },
          "400": {
            "description": "Invalid request",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          }
        }
      }
    },
    "/ws/recorddata/update": {
      "post": {
        "description": "Force update record data for a specific ID",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "WebSocket-RecordData"
        ],
        "summary": "Update record data",
        "parameters": [
