  "paths": {
    "/ws/recorddata/gotopage": {
      "post": {
        "description": "Jump record data to a specific page",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "WebSocket-RecordData"
        ],
        "summary": "Go to specific page",
        "parameters": [
          {
            "description": "Page control request",
            "name": "message",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/controller.RecordControlPageReceive"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Page switched",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          },
          "400": {
            "description": "Invalid request",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          }
        }
      }
    },
    "/ws/recorddata/nextpage": {
      "post": {
        "description": "Switch record data to next page",
