            }
          }
        }
      }
    },
    "/ws/recorddata/register": {
      "post": {
        "description": "Register a client to receive record data updates via WebSocket",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "WebSocket-RecordData"
        ],
        "summary": "Register record data polling",
        "parameters": [
          {
            "description": "Register request payload",
            "name": "message",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/controller.RecordRegisterReceive"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Register success response",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          },
          "400": {
            "description": "Invalid request",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          }
        }
      }
    },
    "/ws/recorddata/unregister": {
      "post": {
        "description": "Unregister a client from record data polling",
        "consumes": [
          "application/json"
        ],
        "produces": [
