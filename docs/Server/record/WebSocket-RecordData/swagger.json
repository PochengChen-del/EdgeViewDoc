          },
          "400": {
            "description": "Invalid request",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          }
        }
      }
    },
    "/ws/recorddata/prevpage": {
      "post": {
        "description": "Switch record data to previous page",
