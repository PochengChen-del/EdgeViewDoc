  "paths": {
    "/ws/login/login": {
      "post": {
        "description": "Authenticate client via WebSocket using CSRF token",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "tags": [
          "WebSocket-Login"
        ],
        "summary": "WebSocket login",
        "parameters": [
          {
            "description": "Login request payload",
            "name": "message",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/wsmodel.Login"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Login success",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            },
            "examples": {
              "application/json": {
                "action": "login",
                "error": 0,
                "reason": "",
                "topic": "login"
              }
            }
          },
          "400": {
            "description": "Invalid request",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/wsmodel.TopicMessage"
            }
          }
        },
        "x-example": {
          "action": "login",
          "error": 0,
          "reason": "",
          "topic": "login"
        }
      }
    }
  },
